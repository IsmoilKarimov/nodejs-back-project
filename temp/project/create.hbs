<div class="main-content project">
    <div class="row">
        <div class="col-12">
            <form action="/project/add" method="post" enctype="multipart/form-data">
                <div class="box">
                    <div class="box-body">
                        <div class="row">
                            <div class="col-md-12 col-sm-12 mb-24">
                                <div class="form-group"> 
                                    <label class="form-label">Project Sarlavhasi</label> 
                                    <input name="title" class="form-control" placeholder="Software Development"> 
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-md-6 col-sm-12 mb-24">
                                <div class="form-group"> 
                                    <label class="form-label">Bo'limlar:</label> 
                                    <select
                                        name="department"
                                        class="form-control custom-select select2 select2-hidden-accessible"
                                        data-placeholder="Bo'limni tanlang" tabindex="-1" aria-hidden="true"
                                        data-select2-id="select2-data-22-9i9m">
                                        <option label="Bo'limni tanlang" data-select2-id="select2-data-24-ktnv"></option>
                                        {{#each departments}}
                                        <option value="{{_id}}">{{title}}</option>
                                        {{/each}}
                                    </select>
                                </div>
                            </div>
                            <div class="col-md-6 col-sm-12 mb-24">
                                <div class="form-group"> 
                                    <label class="form-label">Project Priority:</label> <select
                                        name="priority"
                                        class="form-control custom-select select2 select2-hidden-accessible"
                                        data-placeholder="Select Priority" tabindex="-1" aria-hidden="true"
                                        data-select2-id="select2-data-30-l6ni">
                                        <option label="Priorityni tanlang" data-select2-id="select2-data-32-qodq"></option>
                                        <option value="1">Yuqori</option>
                                        <option value="2">O'rta</option>
                                        <option value="3">Past</option>
                                    </select>
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-md-6 col-sm-12 mb-24">
                                <div class="form-group"> 
                                    <label class="form-label">Mijozlar:</label> 
                                    <select name="cleint"
                                        class="form-control custom-select select2 select2-hidden-accessible"
                                        data-placeholder="Mijozni tanlang" tabindex="-1" aria-hidden="true"
                                        data-select2-id="select2-data-38-9jkg">
                                        <option label="Mijozni tanlang" 
                                        data-select2-id="select2-data-40-q3xu"></option>
                                        {{#each clients}}
                                        <option value="{{_id}}">{{name}}</option>
                                        {{/each}}
                                    </select>
                                </div>
                            </div>
                            <div class="col-md-6 col-sm-12 mb-24">
                                <div class="form-group"> 
                                    <label class="form-label">Narh ( $ )</label> 
                                    <input name="price" class="form-control" placeholder="Narhni kiriting"> 
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-md-6 col-sm-12 mb-24">
                                <div class="form-group "> 
                                    <label class="form-label">Guruhlar:</label> 
                                    <select
                                        name="team"
                                        class="form-control custom-select select2 select2-hidden-accessible"
                                        data-placeholder="Guruhni tanlang" tabindex="-1" aria-hidden="true"
                                        data-select2-id="select2-data-38-9jkg">
                                        <option label="Guruhni tanlang" data-select2-id="select2-data-40-q3xu"></option>
                                        {{#each teams}}
                                        <option value="{{_id}}">{{name}}</option>
                                        {{/each}}
                                    </select>
                                </div>
                            </div>
                            <div class="col-md-3 col-sm-12 mb-24">
                                <div class="form-group"> 
                                    <label class="form-label">From:</label>
                                    <div class="input-group">
                                        <div class="input-group-prepend">
                                            <div class="input-group-text"> 
                                                <i class="bx bx-calendar"></i> 
                                            </div>
                                        </div>
                                        <input name="from" class="form-control fc-datepicker" placeholder="DD-MM-YYY" type="date">
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-3 col-sm-12 mb-24">
                                <div class="form-group"> 
                                    <label class="form-label">To:</label>
                                    <div class="input-group">
                                        <div class="input-group-prepend">
                                            <div class="input-group-text"> 
                                                <i class="bx bx-calendar"></i> 
                                            </div>
                                        </div>
                                        <input name="to" class="form-control fc-datepicker" placeholder="DD-MM-YYY" type="date">
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="form-group mb-24"> 
                            <label class="form-label">Description:</label>
                            <textarea name="description" class="form-control" name="text" cols="30" rows="10"></textarea>
                        </div>
                        <div class="row">
                            <div class="col-md-6 col-sm-12">
                                <div class="form-group"> 
                                    <label class="form-label">Attachment:</label>
                                    <div class="input-group file-browser"> 
                                        <input type="text" name="attachment" class="form-control border-end-0 browse-file" placeholder="choose" readonly="">
                                        <label class="input-group-append mb-0"> 
                                            <span class="btn ripple btn-light"> Browse
                                                <input type="file" class="file-browserinput" style="display: none;" multiple=""> 
                                            </span> 
                                        </label> 
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-6 col-sm-12">
                                <div class="custom-controls-stacked d-lg-flex align-items-center">
                                    <label class="form-label mt-1 fs-18 font-w500 color-primary">Work Status :</label>
                                    <label class="custom-control custom-radio success me-4">
                                        <input type="radio" class="custom-control-input" name="status"
                                            value="option1">
                                        <span class="custom-control-label">Completed</span>
                                    </label>
                                    <label class="custom-control custom-radio success me-4">
                                        <input type="radio" class="custom-control-input" name="status"
                                            value="option2">
                                        <span class="custom-control-label">Pending</span>
                                    </label>
                                    <label class="custom-control custom-radio success me-4">
                                        <input type="radio" class="custom-control-input" name="status"
                                            value="option3">
                                        <span class="custom-control-label">On Progress</span>
                                    </label>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="gr-btn mt-15"> 
                        <button type="submit" class="btn btn-danger btn-lg mr-15 fs-16">CLOSE</button> 
                        <button type="submit" class="btn btn-primary btn-lg fs-16">SUBMIT</button>
                    </div>
                </div>
            </form>
        </div>
    </div>
</div>